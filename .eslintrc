{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "indent": "off",
        "linebreak-style": "off",
        "comma-dangle": "off",
        "semi": "off",
        "arrow-parens":"off",
        "eol-last":"off",
        "object-curly-spacing":"off",
        "require-jsdoc":"off",
        "operator-linebreak":"off",
    },
    "extends": ["eslint:recommended", "google"]
}